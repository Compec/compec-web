  "scripts": {
    "ng": "ng",
    "start": "npm run build:ssr && npm run serve:ssr",
    "build": "ng build",
    "sw-build": "ng build --prod && node build/fix-sw",
    "sw-build-live": "ng build --prod --base-href http://localhost:4000 && node build/fix-sw",
    "lint": "ng lint",
    "build:client-and-server-bundles": "ng build --prod && ng run compec-web:server",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server && node build/fix-sw",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "pre-commit": [],
